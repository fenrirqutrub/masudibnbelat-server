{
  "version": 2,
  "builds": [
    {
      "src": "main.go",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.go",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }
  ],
  "functions": {
    "index.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "env": {
    "MONGODB_URI": "@mongodb_uri"
  }
}
